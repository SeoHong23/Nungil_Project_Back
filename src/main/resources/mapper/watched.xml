    <!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
            "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

    <mapper namespace="com.nungil.Repository.Interfaces.WatchedRepository">

        <insert id="insertWatched">
            INSERT INTO watched (user_id, video_id, created_at)
            VALUES (#{userId}, #{videoId}, #{createdAt})
        </insert>


    </mapper>
